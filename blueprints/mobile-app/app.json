{
  "expo": {
    "name": "Campaign Express",
    "slug": "campaign-express-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#2563eb"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.campaignexpress.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#2563eb"
      },
      "package": "com.campaignexpress.mobile"
    },
    "plugins": [
      "expo-notifications",
      "expo-location",
      "expo-secure-store"
    ]
  }
}
